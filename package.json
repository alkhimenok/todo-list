{
	"name": "todo-list",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-scripts": "5.0.0",
		"reset-css": "^5.0.1",
		"styled-components": "^5.3.3",
		"uid": "^2.0.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format": "npx prettier --write .",
		"lint": "eslint --ignore-path .gitignore .",
		"lint:fix": "eslint --fix ."
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*": "your-cmd",
		".js": ["npm run format", "npm run lint:fix", "git add"]
	},
	"devDependencies": {
		"eslint": "^8.8.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"lint-staged": "^12.3.3",
		"prettier": "2.5.1"
	}
}
